{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ESNext",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "removeComments": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["docs/.vuepress/*.ts"],
  "exclude": ["node_modules", "dist"]
}
